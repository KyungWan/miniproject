;AADA;AArCA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n    <h2>Detailed Board List</h2>\n    <board-read v-if=\"board\" :board=\"board\"/>\n    <p v-else>Loading ...</p>\n    <router-link :to=\"{ name: 'BoardModifyPage', params: { boardNo } }\">\n      Edit\n    </router-link>\n    <button @click=\"onDelete\">Delete</button>\n    <router-link :to=\"{ name: 'BoardListPage' }\">\n      List\n    </router-link>\n  </div>\n</template>\n\n<script>\nimport BoardRead from '@/components/BoardRead'\nimport { mapState, mapActions } from 'vuex'\nimport axios from 'axios'\n\nexport default {\n  name: 'BoardReadPage',\n  props: {\n    boardNo: {\n      type: String,\n      required: true\n    }\n  },\n  computed: {\n    ...mapState([\n      'board'\n    ])\n  },\n  created () {\n    console.log('BoardReadPage created(): ' + this.boardNo)\n    this.fetchBoard(this.boardNo)\n      .catch(err => {\n        alert(err.response.data.message)\n        this.$router.push()\n      })\n  },\n  methods: {\n    ...mapActions([\n      'fetchBoard'\n    ]),\n    onDelete () {\n      const { boardNo } = this.board\n      axios.delete(`http://localhost:7777/boards/${boardNo}`)\n        .then(res => {\n          alert('Delete Success')\n          this.$router.push({ name: 'BoardListPage' })\n        })\n        .catch(err => {\n          alert(err.response.data.message)\n        })\n    }\n  },\n  components: {\n    BoardRead\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}