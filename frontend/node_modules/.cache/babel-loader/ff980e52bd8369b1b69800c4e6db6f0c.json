CAAA;AACA,UAAA,IAAA,EAAA,eADA;AAEA,UAAA,MAAA,EAAA;AAAA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,QAAA;AAAA;AAFA,SAAA;AAIA,OARA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,OAXA;AAYA;AAhBA,KAiBA,UAAA,CAAA,CACA,YADA,CAAA,CAjBA;AAtBA,CAAA","sourcesContent":["<template>\n  <div align=\"center\">\n    <h2>Board Modification</h2>\n    <board-modify-form v-if=\"board\" :board=\"board\" @submit=\"onSubmit\"/>\n    <p v-else>Loading ...</p>\n  </div>\n</template>\n\n<script>\nimport BoardModifyForm from '@/components/BoardModifyForm'\nimport { mapState, mapActions } from 'vuex'\nimport axios from 'axios'\n\nexport default {\n  name: 'BoardModifyPage',\n  components: {\n    BoardModifyForm\n  },\n  props: {\n    boardNo: {\n      type: String,\n      required: true\n    }\n  },\n  computed: {\n    ...mapState(['board'])\n  },\n  created () {\n    console.log('BoardModifyPage created()')\n    this.fetchBoard(this.boardNo)\n      .catch(err => {\n        alert(err.response.data.message)\n        this.$router.back()\n      })\n  },\n  methods: {\n    onSubmit (payload) {\n      const { title, content } = payload\n      console.log('BoardModifyPage payload: ' + payload)\n      axios.put(`http://localhost:7777/boards/${this.boardNo}`, { title, content })\n        .then(res => {\n          alert('Modify Success')\n          console.log('res: ' + res.data)\n          this.$router.push({\n            name: 'BoardReadPage',\n            params: { boardNo: res.data.boardNo.toString() }\n          })\n        })\n        .catch(err => {\n          alert(err.response.data.message)\n        })\n    },\n    ...mapActions([\n      'fetchBoard'\n    ])\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}